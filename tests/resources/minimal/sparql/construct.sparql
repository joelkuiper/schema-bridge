PREFIX field: <https://catalogue.org/field/>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>

CONSTRUCT {
  ?res a dcat:Dataset ;
    dct:title ?name ;
    dct:description ?description .
}
WHERE {
  ?res field:name ?name .
  OPTIONAL { ?res field:description ?description }
}
