PREFIX field: <https://catalogue.org/field/>
PREFIX entity: <https://catalogue.org/entity/>

SELECT
  ?id
  ?name
  ?desc
  ?website
  ?email
WHERE {
  ?res a entity:Resource ;
       field:id ?id ;
       field:name ?name ;
       field:description ?desc .
  OPTIONAL { ?res field:website ?website . }
  OPTIONAL { ?res field:contactEmail ?email . }
}
